<h1> Listing <%= Dojo.count %> Dojos</h1>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <td>Coding Dojo Branch</td>
            <td>Street</td>
            <td>City</td>
            <td>State</td>
            <td>Actions</td>
        </tr>
    </thead>
    <% @dojos.each do |dojo| %>
    <tr>
        <td><%= dojo.branch %></td>
        <td><%= dojo.street %></td>
        <td><%= dojo.city %></td>
        <td><%= dojo.state %></td>
        <td>
        <a href="/dojos/<%= dojo.id %>">Show</a> | 
        <a href="/dojos/<%= dojo.id %>/edit">Edit</a> | 
        <a href="/dojos/<%= dojo.id %>" data-method="DELETE">Destroy</a>
        </td>
    </tr>
    <% end %>
</table>
<a href="/dojos/new">Add a New Dojo</a> 
